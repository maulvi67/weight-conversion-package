{
    "name": "maulvi67/weight-conversion-package",
    "description": "This is my package weight-conversion-package",
    "keywords": [
        "maulvi67",
        "weight-conversion-package"
    ],
    "homepage": "https://github.com/maulvi67/weight-conversion-package",
    "license": "MIT",
    "authors": [
        {
            "name": "maulvi",
            "email": "maulvi67@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Maulvi67\\WeightConversionPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maulvi67\\WeightConversionPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
